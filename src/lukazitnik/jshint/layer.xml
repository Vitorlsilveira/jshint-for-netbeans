<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="TaskList">
        <folder name="Groups">
            <file name="jshint.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                <attr name="groupName" stringvalue="lukazitnik-tasklist-jshint"/>
                <attr name="diplayNameKey" stringvalue="LBL_jshint_group"/>
                <attr name="descriptionKey" stringvalue="HINT_jshint_group"/>
                <attr name="iconKey" stringvalue="ICON_jshint"/>
            </file>
        </folder>
    </folder>
    <folder name="Scanners">
        <file name="JSHintFileTaskScanner.instance">
            <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.FileTaskScanner"/>
            <attr name="instanceCreate" methodvalue="lukazitnik.JSHintFileTaskScanner.create"/>
        </file>
    </folder>
</filesystem>
